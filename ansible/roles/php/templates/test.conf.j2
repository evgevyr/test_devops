[{{ pool_name }}]

user = {{ pool_user }}
group = {{ pool_group }}

listen = {{ php_socket }}

listen.owner = {{ listen_owner }}
listen.group = {{ listen_group }}
listen.mode = {{ listen_mode }}

pm = {{ pm }}

pm.max_children = {{ pm_max_children }}
pm.start_servers = {{ pm_start_servers }}
pm.min_spare_servers = {{ pm_min_spare_servers }}
pm.max_spare_servers = {{ pm_max_spare_servers }}

pm.max_requests = {{ pm_max_requests }}

php_flag[display_errors] = {{ php_flag_display_errors }}
php_admin_value[error_log] = {{ php_admin_value_error_log }}
php_admin_flag[log_errors] = {{ php_admin_flag_log_errors }}
php_admin_value[memory_limit] = {{ php_admin_value_memory_limit }}